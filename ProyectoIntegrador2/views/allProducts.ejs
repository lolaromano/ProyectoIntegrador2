<%- include("partials/header.ejs") %> 

<div class="banner">
    <img src="images/jordan/banner8.png" alt="Banner de ofertas"> 
</div>

<div class="container products-wrapper">
	<div class="row">
		<div class="col-12">
			<h2 class="products-title">M√ÅS COMENTADOS</h2>
		</div>
	<!-- 	Show the "ultimos" products -->
		<% for (let i=4; i<8; i+=1 ) { %>
		<div class="col-12 col-sm-6 col-lg-3">
			<section class="product-box">
				<a href="/products">
					<figure class="product-box_image">
						<img src="<%= productos.lista[i].imagen %>">
					</figure> 
					<article class="product-box_data">
						<h2> <%= productos.lista[i].nombre %> </h2>
						<p> <%= productos.lista[i].modelo %> </p> 
					
	
					</article>
				</a>
			</section> 
		</div> 
		<%}%> 
	</div>
</div> 

<%- include("partials/footer.ejs") %> 